{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/quote-tool.html"
    },
    {
      "src": "/quote-tool",
      "dest": "/quote-tool.html"
    },
    {
      "src": "/payment-form",
      "dest": "/payment-form.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
