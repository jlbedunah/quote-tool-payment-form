{
  "name": "quote-tool-payment-form",
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/",
      "dest": "/quote-tool.html"
    },
    {
      "src": "/quote-tool",
      "dest": "/quote-tool.html"
    },
    {
      "src": "/payment-form",
      "dest": "/payment-form.html"
    },
    {
      "src": "/payment-form.html",
      "dest": "/payment-form.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
