{
  "installCommand": "npm install",
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/quotes",
      "destination": "/quotes.html"
    },
    {
      "source": "/payment-success",
      "destination": "/payment-success.html"
    }
  ]
}